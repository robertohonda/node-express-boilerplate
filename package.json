{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a API with Node.js, Express.",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run clean && npm run build && node ./build/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "clean": "rm -rf build && mkdir build",
    "build": "npm install && babel -d ./build ./src -s"
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "http-status": "^1.3.2",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.1"
  }
}
